from desmos2python.latex import DesmosLatexParser
from desmos2python.browser import DesmosWebSession


def make_latex_parser(auto_exec=False, auto_init=False, **kwds):
    return DesmosLatexParser(auto_exec=auto_exec, auto_init=auto_init, **kwds)


def make_web_session(url, **kwds):
    return DesmosWebSession(url=url)
